MAIL_MAILER=smtp
MAIL_HOST=mailhog
MAIL_PORT=1025
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS=null
MAIL_FROM_NAME="${APP_NAME}"

# 系统通知邮箱地址（数据备份，异常等）
MAIL_NOTIFICATION_ADDRESS=null

SCOUT_DRIVER=meilisearch
